{% extends 'base.html.twig' %}

{% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/stylenotes.css') }}">    
{% endblock %}
{% block javascripts %}
        <script src="{{ asset('js/script.js') }}"></script>
{% endblock %}

{% block body %}
    <nav class="navbar">
        <div class="container">
            <a class="navbar-brand" href="#">
                <img src="data:image/png;base64," alt="Logo" width="100" height="50" class="d-inline-block align-text-top">
            </a>
            <p id="userid">{{ name }}</p>
            <div class="button">
                <a class="btn btn-primary" href="/editprofile">Edit Profile</a>
                <button class="btn btn-outline-success" id="logout" onclick="logout()" type="submit">Logout</button>
            </div>
        </div>
    </nav>
    <div class="container">
        <div class="post">
        {% for item in post %}
        {% if name == item.username %}
        <div class="inner-post" id="post-{{item.id}}">
            <p>{{item.username}}</p>
            <p>{{item.title}}</p>
            <p id="posttext-{{item.id}}">{{substr(item.post,0,100)}}</p>
        </div>
        {% endif %}
        {% endfor %}
        </div>
    </div>
{% endblock %}    